int solution(vector<int> &A) {
	// write your code in C++11
	int sum = 0;
	int newsum = 0;
	int min = 999999;
	int N = A.size();
	
	for(int i=0; i<N; i++)
	{
    	sum = sum+A[i];
	}
	for(int i=1; i<N; i++)
	{
    		sum = sum-A[i-1];
    		newsum = newsum+A[i-1];
    		if(abs(sum-newsum) < min)
    		{
        		min = abs(sum-newsum);
    		}
	}
	return min;
}
